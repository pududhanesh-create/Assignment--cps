n1=float(input("choose your first number :"))
n2=float(input("choose your second number :"))
add=n1+n2
sub=n1-n2
mul=n1*n2
div=n1/n2
rem=n1%n2
intquo=n1//n2
print("The sum of the two numbers :",add)
print("The subtraction of the two numbers :",sub)
print("The multiplication of the two numbers :",mul)
print("The division of two numbers :",div)
print("The remainder of two numbers13 while dividing :",rem)
print("The  intiger quotient of the two number of dividing :",intquo)
